function baseAssignValue(object, key, value) {
  if (key == '__proto__' && require('./_defineProperty')) {
    require('./_defineProperty')(object, key, {
      'configurable': true,
      'enumerable': true,
      'value': value,
      'writable': true });
  } else {
    object[key] = value;
  }
}

module.exports = baseAssignValue;